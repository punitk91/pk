class D 
{
	public static void main(String[] args) 
	{
	  String s1="Punit Kulkarni";
	  StringBuffer sb2 = new StringBuffer(s1);
	  System.out.println(sb2.reverse());
	  System.out.println("Lenght of the string = " +sb2.length());


	  StringBuffer sb3 = new StringBuffer("Durga");
	  String ss = sb3.toString();
	  System.out.println(ss);
	}
}
